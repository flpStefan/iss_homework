<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="433.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="iss.bug_application.controller.LoginController">
    <children>
        <ImageView fitHeight="400.0" fitWidth="601.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@login.jpeg" />
            </image>
        </ImageView>
        <TextField fx:id="username" layoutX="168.0" layoutY="102.0" prefHeight="42.0" prefWidth="162.0" promptText="Username" />
        <PasswordField fx:id="password" layoutX="169.0" layoutY="156.0" prefHeight="42.0" prefWidth="162.0" promptText="Password" />
        <Button fx:id="login" layoutX="202.0" layoutY="261.0" mnemonicParsing="false" onAction="#handle_login" prefHeight="26.0" prefWidth="94.0" text="Log In" />
      <ComboBox fx:id="function_box" layoutX="203.0" layoutY="214.0" prefHeight="26.0" prefWidth="94.0" promptText="Function" />
    </children>

</AnchorPane>
